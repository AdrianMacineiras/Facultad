package ejercicio3;

import org.junit.Test;
import static org.junit.Assert.*;

public class PosicionTest {
      
    /**
     * Test of getFila method, of class Posicion.
     */
    @Test
    public void testGetFila() {
        Posicion posicion = new Posicion('a', '1');        
        assertEquals('1', posicion.getFila());
    }

    /**
     * Test of getColumna method, of class Posicion.
     */
    @Test
    public void testGetColumna() {
        Posicion posicion = new Posicion('a', '1');        
        assertEquals('a', posicion.getColumna());
    }
}
